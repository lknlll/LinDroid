<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lindroidcode">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".networkstate.NetworkStateActivity"></activity>
        <activity android:name=".constraintdemo.ConstraintActivity" />
        <activity android:name=".textviewrelated.TextViewDemoActivity" />
        <activity android:name=".customizecircleprogress.CircleProgressActivity" />
        <activity android:name=".xmldemo.XmlDemoActivity" />
        <activity android:name=".inputstream.InputOutputStreamActivity" />
        <activity android:name=".canvas.CanvasActivity" />
        <activity
            android:name=".activityrelated.lifecycle.BActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".activityrelated.lifecycle.AActivity"
            android:launchMode="singleTop" />
        <activity android:name=".activityrelated.LearnActivityActivity" />
        <activity android:name=".recyclerrelated.RecyclerConfigActivity" />
        <activity android:name=".recyclerrelated.RecyclerRelatedActivity" />
        <activity android:name=".leastsquares.LineChartActivity" />
        <activity
            android:name=".permission.HandlePermissionActivity"
            android:label="HandlePermissionActivity"
            android:theme="@style/activity_translucent" />
        <activity
            android:name=".permission.ApplyPermissionActivity"
            android:label="ApplyPermissionActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".sensors.activities.ShakeActivity" />
        <activity android:name=".mvp.activities.UserLoginActivity" />
    </application>

</manifest>